{
  "name": "address-extractor",
  "author": "Pierre Donias",
  "license": "AGPL-3.0",
  "version": "0.2.0",
  "homepage": "https://github.com/pdonias/address-extractor#readme",
  "description": "Extracts addresses from a text",
  "preferGlobal": false,
  "main": "dist/",
  "bin": {},
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=dist/ src/",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=dist/ src/",
    "dev-test": "jest --watch",
    "lint": "standard",
    "posttest": "npm run lint",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          }
        }
      ],
      "stage-3"
    ]
  },
  "pre-commit": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdonias/address-extractor.git"
  },
  "keywords": [
    "address",
    "extract",
    "find"
  ],
  "bugs": {
    "url": "https://github.com/pdonias/address-extractor/issues"
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-stage-3": "^6.16.0",
    "cross-env": "^3.1.4",
    "jest": "^18.1.0",
    "pre-commit": "^1.2.2",
    "standard": "^8.6.0"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
